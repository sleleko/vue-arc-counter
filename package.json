{
  "name": "vue-arc-counter",
  "version": "2.0.0",
  "description": "Vue compoment to create an SVG counter in a circular arc",
  "author": {
    "name": "Roy Prins",
    "email": "prinsroy@gmail.com"
  },
  "repository": "https://github.com/snirp/vue-arc-counter",
  "bugs": "https://github.com/snirp/vue-arc-counter/issues",
  "keywords": [
    "vue",
    "arc",
    "counter",
    "svg",
    "circle"
  ],
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:npm": "vue-cli-service build --target lib --name arcCounter ./src/ArcCounter.vue"
  },
  "main": "dist/arcCounter.umd.js",
  "unpkg": "dist/arcCounter.umd.min.js",
  "dependencies": {
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
